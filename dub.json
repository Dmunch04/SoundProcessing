{
    "name": "sound-processing",
    "description": "Processes sounds",
    "authors": ["Munchii"],
    "license": "MIT",
    "homepage": "https://github.com/Dmunch04/SoundProcessing",

    "dependencies": {
        "dlib": "~>0.17.0"
    },
    "importPaths": [
        "."
    ],
    "buildRequirements": [
        "allowWarnings"
    ],
    "lflags-linux-gdc": ["-lz"],
    "configurations": [
        {
            "name": "library",
            "targetType": "library",
            "sourcePaths": ["soundprocessing"]
        },
        {
            "name": "import",
            "targetType": "sourceLibrary",
            "sourceFiles-posix": ["libsound-processing.a"],
            "sourceFiles-windows": ["sound-processing.lib"]
        }
    ]
}